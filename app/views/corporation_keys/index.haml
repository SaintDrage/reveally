%h1 Corporations list

%table
  %thead
    %tr
      %th Id
      %th v_code
      %th corporation_id
  %tbody
    - @corporation_keys.each do |key|
      %tr
        %td= link_to key.id, edit_corporation_key_path(key)
        %td= key.v_code
        %td= link_to key.corporation.name, "/corporation/#{key.corporation.id}"
        %td= button_to 'Delete', corporation_key_path(key), :method => :delete, data: {confirm: 'Are you sure?'}
